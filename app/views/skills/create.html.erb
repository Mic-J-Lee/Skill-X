<h1>Create New Skill</h1>
<p><%= form_for(@post) do |f| %>
      <%= f.label :skill %>
      <%= f.text_field :skill, class: 'form-control' %>
      <%= f.label :description %>
      <%= f.text_field :description, class: 'form-control' %>
      <%= f.submit "Save Changes", class: "btn btn-primary" %>
      <% end %>